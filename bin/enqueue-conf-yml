#!/usr/bin/env perl
$\ = "\n";

use YAML;
use FindBin;
BEGIN { unshift @INC, "$FindBin::Bin/../lib"; }

use EnQ::Ast::AutoIVR;
use EnQ::Ast::Groups;

exit if !$ARGV[0];
exit if !$ARGV[1];

my $y = YAML::LoadFile($ARGV[0]);

foreach(@{$y->{'List'}}){
    &{$EnQ::Ast::{$_->[0].'::'}{$ARGV[1]}}(
	YAML::LoadFile($_->[1])
    );
}
