#!/usr/bin/env perl
$\ = "\n";

use YAML;
use FindBin;
BEGIN { unshift @INC, "$FindBin::Bin/../lib"; }

use EnQ::Ast::AutoIVR;

exit if !$ARGV[0];
my $y = YAML::LoadFile($ARGV[0]);

print " --- EnQ::Ast::AutoIVR::conf_queues --- ";
EnQ::Ast::AutoIVR::conf_queues($y);

print " --- EnQ::Ast::AutoIVR::conf_extensions_landing --- ";
EnQ::Ast::AutoIVR::conf_extensions_landing($y);

print " --- EnQ::Ast::AutoIVR::conf_extensions_agent --- ";
EnQ::Ast::AutoIVR::conf_extensions_agent($y);

print " --- EnQ::Ast::AutoIVR::conf_extensions_default --- ";
EnQ::Ast::AutoIVR::conf_extensions_default($y);

print " --- EnQ::Ast::AutoIVR::conf_extensions_global --- ";
EnQ::Ast::AutoIVR::conf_extensions_global($y);
